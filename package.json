  "name": "hotel-admin",
  "version": "1.0.0",
  "description": "vue",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --mode development  --host 0.0.0.0 --config ./build/webpack.dev.config.js --progress --colors --inline",
    "build": "npm run clean && cross-env NODE_ENV=prod webpack --mode production --config  ./build/webpack.pro.config.js --progress --colors",
    "build:dll": "webpack --config ./build/webpack.dll.config.js",
    "dev": "cross-env NODE_ENV=dev node ./build/server-dev.js --mode development",
    "prod": "npm run clean && cross-env NODE_ENV=prod node ./build/server-prod.js --mode production",
    "clean": "shx rm -rf dist",
    "lint": "eslint --ignore-path .eslintignore --ext .vue,.js ./src",
    "fix": "eslint --fix --ext .vue,.js ./src",
    "test:e2e": "node ./test/e2e/runner.js"
  },
